    Public Property Destination As Integer
        Get
            Return Me._Volume
        End Get
        Set(ByVal value As Integer)
            If (value <= Maxnum) Then
                Me._Volume = value
            Else
                Me._Volume = Maxnum
            End If
            If (value < 0) Then
                Me._Volume = 0
            End If
            Me.Invalidate()
        End Set
